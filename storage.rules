rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    
    // Your Admin UID
    function isAdmin() {
       return request.auth.uid == "43kKa0OSNySADEVcIKWucnAiHPo1"; 
    }

    match /blog-images/{allPaths=**} {
      allow read: if true;
      allow write: if isAdmin();
    }
  }
}
